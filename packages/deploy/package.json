{
  "name": "@tnnevol/deploy",
  "version": "5.2.5",
  "description": "部署工具",
  "type": "module",
  "scripts": {
    "start": "pnpm run start:rollup",
    "build": "pnpm run build:rollup",
    "start:rollup": "rimraf ./dist && rollup --config rollup.config.ts -w --configPlugin typescript",
    "build:rollup": "rimraf ./dist && rollup --config rollup.config.ts --configPlugin typescript"
  },
  "bin": {
    "deploy": "./bin/index.mjs"
  },
  "keywords": [
    "deploy"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./types/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "bin",
    "dist",
    "types"
  ],
  "author": "batter-vae <greecenew@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "git@gitee.com:tnnevol/tnnevol_tools.git"
  },
  "license": "MIT",
  "dependencies": {
    "@tnnevol/register-cli": "workspace:*",
    "archiver": "^5.3.1",
    "chalk": "^4.1.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "dedent": "^0.7.0",
    "inquirer": "^8.1.1",
    "nanoid": "^4.0.1",
    "node-ssh": "^11.0.0",
    "ora": "^5.1.0",
    "tasksfile": "^5.1.1"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.2",
    "@types/crypto-js": "^4.1.1",
    "@types/dedent": "^0.7.0",
    "@types/inquirer": "^8.1.1",
    "@types/node": "^14.18.37",
    "@types/node-ssh": "^7.0.1",
    "rollup": "^3.2.3",
    "typescript": "^4.8.4"
  }
}
