{
  "name": "@tnnevol/register-cli",
  "version": "5.2.6",
  "description": "注册 cli",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "start": "pnpm run start:rollup",
    "build": "pnpm run build:rollup",
    "start:rollup": "rimraf ./dist && rollup --config rollup.config.ts -w --configPlugin typescript",
    "build:rollup": "rimraf ./dist && rollup --config rollup.config.ts --configPlugin typescript"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "keywords": [
    "register-cli"
  ],
  "files": [
    "dist",
    "test",
    "build.config.ts"
  ],
  "author": "batter-vae <greecenew@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "git@gitee.com:tnnevol/tnnevol_tools.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "tasksfile": "^5.1.1"
  },
  "dependencies": {
    "fs-extra": "^11.1.0",
    "@types/fs-extra": "^11.0.1",
    "tasksfile": "^5.1.1",
    "rollup": "^3.2.3"
  },
  "devDependencies": {
    "typescript": "^4.8.4"
  }
}
