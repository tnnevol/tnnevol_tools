{
  "extends": "ts-config/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./dist",
    "typeRoots": [
      "node_modules/@types",
      "./types"
    ],
    "types": ["./types/index"],

    "paths": {
      "alioss": ["./types/alioss"]
    }
  },
  "typeAcquisition": {
    "enable": true
  },
  "include": [
    "src/**/*"
  ]
}
